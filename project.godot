config_version=5

[application]

config/name="Homeless Hubert's Hamburger Hunt"
run/main_scene="res://prefabs/main_dock.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
PlayerAutoload="*res://autoloads/player_autoload.gd"
GlobalsAutoload="*res://autoloads/globals_autoload.gd"
BattleAutoload="*res://autoloads/battle_autoload.gd"

[dialogic]

directories/dch_directory={
"unit_test_character": "res://Tests/Resources/unit_test_character.dch"
}
directories/dtl_directory={}

[display]

window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/YouCanDoIt/plugin.cfg", "res://addons/dialogic/plugin.cfg")

[file_customization]

folder_colors={
"res://Resources/": "red",
"res://art/": "orange",
"res://autoloads/": "gray"
}

